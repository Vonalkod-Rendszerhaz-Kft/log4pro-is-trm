<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework"
      type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
      requirePermission="false"/>
  </configSections>
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <appSettings>
    <add key="VRH.XmlParser:root" value="~XmlParser.xml"/>
    <add key="Vrh.Logger:Config" value="file=~LogConfig.xml"/>
    <add key="Vrh.ApplicationContainer:ConfigurationFile" value="file=~Plugins.Config.xml"/>
    <add key="Vrh.ApplicationContainer.InstanceFactory.FromXML:ConfigurationFile" value="file=~Plugins.Config.xml"/>
    <add key="VRH.ConnectionStringStore" value="VRHConnections.xml" />
  </appSettings>
</configuration>
